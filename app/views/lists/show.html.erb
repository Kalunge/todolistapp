<h1> <%= @list.name %> </h1>

<% @list.todos.each do | todo | %>
<div>
  <h2>Todos</h2>
  <p><%= todo.description %></p>
  <p><%= todo.status %></p>
</div>
<% end %>


<%= form_with model: @todo do |form| %>
<div>
  <%= form.label :name %><br>
  <%= form.text_field :name %>

  <% @todo.errors.full_messages_for(:name).each do |message| %>
  <div><%= message %></div>
  <% end %>
</div>

<div>
  <%= form.submit %>
</div>
<% end %>
<form action="">

</form>